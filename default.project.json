{
  "$schema": "https://rojo.space/schemas/7.0.0.json",
  "name": "ROBOT_COLLECTORS",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$ignoreUnknownInstances": true,

      "RC_Remotes": {
        "$className": "Folder",
        "$ignoreUnknownInstances": true
      },

      "Client": {
        "$className": "Folder",
        "$path": "src/replicated/Client"
      },

      "Shared": {
        "$path": "src/shared"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$ignoreUnknownInstances": true,
      "Modules":   { "$className": "Folder", "$path": "src/server/Modules" },
      "Services":  { "$className": "Folder", "$path": "src/server/Services" },
      "World":     { "$className": "Folder", "$path": "src/server/world" },
      "Slice2World": { "$path": "src/server/Slice2World.server.luau" },
      "SliceGarageWorld": { "$path": "src/server/SliceGarageWorld.server.luau" },
      "RobotBootstrap": { "$path": "src/server/RobotBootstrap.server.luau" },
      "ServerInit": {
        "$className": "Script",
        "$path": "src/server/init.server.lua",
        "$properties": {
          "RunContext": "Server",
          "Enabled": true
        }
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "$ignoreUnknownInstances": true,
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Client": { "$path": "src/client" }
      }
    }
  }
}
